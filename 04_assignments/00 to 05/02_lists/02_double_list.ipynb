        "# **Write a program that doubles each element in a list of numbers.**"
      ],
      "metadata": {
        "id": "pmVJ9IzaBvPs"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def main():\n",
        "  number_list = [1, 2, 3, 4, 5]\n",
        "\n",
        "  for i in range(len(number_list)):\n",
        "    number_list[i] = number_list[i] * 2\n",
        "\n",
        "  print(number_list)\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    main()"
      ],
